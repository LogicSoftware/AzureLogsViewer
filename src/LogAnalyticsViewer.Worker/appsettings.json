{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "ConnectionString": "Data Source=.;Initial Catalog=lav;Persist Security Info=True;"
  },
  "LogViewer": {
    "DumpSizeInMinutes": 15,
    "DelayBetweenDumpsInMinutes": 5
  },
  "SlackIntegration": {
    "EventUrlFormat": "http://127.0.0.1:81/index?from={From}&to={To}&queryId={QueryId}",
    "ApiToken":  null,
    "RatePerQuery": 100,
    "MessageLength": 350,
    "MessagePattern": "{Date} - {Source}\n{Message}\noccured {Total} times\nsee details {Link}\n__________________________"
  },
  "LogAnalytics": {
    "WorkspaceId": "",
    "ClientId": "",
    "ClientSecret": "",
    "Domain": ""
  }
}
